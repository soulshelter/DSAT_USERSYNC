<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.innoinc.dao.postgres.ir.IrUserMapper">

    <select id="selectAll" resultType="iruser">
        SELECT *
        FROM ir_user
    </select>
    
    <select id="selectIruserByUserid" resultType="iruser">
        SELECT *
        FROM ir_user
        WHERE user_id = #{userid}
    </select>

</mapper>